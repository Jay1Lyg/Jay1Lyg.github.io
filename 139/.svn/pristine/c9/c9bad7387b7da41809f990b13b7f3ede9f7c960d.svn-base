/*添加新演员*/
.newactor_banner{
    width: 100%;
    position: relative;
    height: 200px;
    overflow: hidden;
    display: flex;
    justify-content: center;
    align-items: flex-start;  
   
    margin:1%;
}
.newactor_banner img{
   width: 100%
}
.newactor_banner div{
    width: 100%;
    position: absolute;
    bottom: 0%;
    background: rgba(0, 0, 0,0.7);

}
.newactor_banner div ul{
   width: 100%;
    
}
.newactor_banner div ul li{

    font-size: 15px;
    text-align: center;
    float: left;
    width: 25%;
    color: white;

}
.newactor_banner div ul li.active{
    display: block;
    color: #4da2ad;
    border-bottom: 2px solid #4da2ad;
}

