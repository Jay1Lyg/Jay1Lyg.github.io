    padding: 0.1rem 1.6rem;
    text-align: center;

}
.weui-skin_android .weui-dialog__ft{
    text-align: center;
}
.weui-skin_android .weui-dialog__btn{
    display: inline;
}
/*弹出层*/
.dialogs{
    display: none;
    width: 80%;
    position: fixed;
    left: 50%;
    top: 10%;
    bottom:20%;
    background: #fdfdfd;
    overflow: auto;
    transform: translate(-50%, 0%);
    z-index: 250;
    animation:myfirst 1s;
    -webkit-animation:myfirst 1s;
    border-radius: 5px;


}
.dialogs_delete{
    display: none;
    width: 80%;
    position: fixed;
    left: 50%;
    transform: translate(-50%, -120%);
    z-index: 101;
    animation:myfirst 1s;
    -webkit-animation:myfirst 1s;
}
.overlay {
    background: #000;
    filter: alpha(opacity=50); /* IE的透明度 */
    opacity: 0.5;  /* 透明度 */
    display: none;
    position: absolute;
    top: 0px;
    left: 0px;
    width: 100%;
    height: 100%;
    z-index: 208; /* 此处的图层要大于页面 */
    display:none;

}
.overlay1 {
    background: #000;
    filter: alpha(opacity=50); /* IE的透明度 */
    opacity: 0.5;  /* 透明度 */
    display: none;
    position: absolute;
    top: 0px;
    left: 0px;
    width: 100%;
    height: 100%;
    z-index: 206; /* 此处的图层要大于页面 */
    display:none;

}
.overlay2 {
    background: #000;
    filter: alpha(opacity=50); /* IE的透明度 */
    opacity: 1;  /* 透明度 */
    display: none;
    position: absolute;
    top: 0px;
    left: 0px;
    width: 100%;
    height: 100%;
    z-index: 206; /* 此处的图层要大于页面 */
    display:none;

}
@keyframes myfirst
{
    from {opacity:0;}
    to {opacity:1;}
}

@-webkit-keyframes myfirst /* Safari and Chrome */
{
    from {opacity:0;}
    to {opacity:1;}
}

