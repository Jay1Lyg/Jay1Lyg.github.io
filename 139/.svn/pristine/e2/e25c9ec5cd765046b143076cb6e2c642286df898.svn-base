*{
	margin:0;
	padding:0;
	list-style:none;
}
html{
	font-size:62.5%;
}
body{
	font-size:12px;
	font-size:1.2rem ;
}
p{
	font-size:14px;
	font-size:1.4rem;
}
.feelCon{
	width:100%;
	background:#ffe6c8;
}
.huawen{
	position:relative;
}
.huawen img{
	width:100%;
	height:15.2rem;
}
.juzuMesg p{
	font-size:2.5rem;
	color:#fff;
	position:relative;
	text-align:center;
	top:-7rem;
}
.juzuTitle{
	width:8rem;
	height:3.1rem;
	margin:-8px auto;
	position:relative;
}
.juzuTitle img{
	width:7.4rem;
	height:2.54rem;
}
.juzuTitle span{
	font-size:1.6rem;
	color:#fff;
	position:absolute;
	top:0.5rem;
	left:1.4rem;
	font-weight:bold;
}
.juzuCon{
	width:100%;
	position:relative;
	padding-left:2rem;
	box-sizing:border-box;
	line-height:22px;
	margin-top:1rem;
}
.juzuCon .messLeft{
	margin-right:1rem;
}
.juzuCon .messLeft,
.juzuCon .messRight{
	float:left;
}
.juzuCon .messLeft ul li,
.juzuCon .messRight ul li{
	font-size:1.4rem;
	color:#f17a3d;
}
.messRight img{
	width:2.8rem;
	height:3.3rem;
	position:absolute;
	top:-2rem;
	right:6rem;
}
.messLeft img{
	width:2.6rem;
	height:3rem;
	position:absolute;
	top:-6rem;
	left:6rem;
}
.juzuMesg{
	margin-top:-4rem;
}
.xiaozhuan{
	width:80%;
	height:3rem;
	border:1px solid #f17a3d;
	border-radius:3px;
	margin:0 auto;
	margin-top:1rem;
}
.roleMesg{
	position:relative;
	margin-top:2rem;
}
.roleMesg span{
	position:absolute;
	font-weight:bold;
}
.bottomBj{
	position:relative;
}
.bottomBj img{
	width:100%;
	height:18rem;
	position:absolute;
}
.erjiTitle{
	width:29rem;
	height:1.125rem;
	margin:1.5rem auto;
}
.erjiTitle img{
	width:29rem;
	height:1.125rem;
}
.erjiTitle p{
	font-size:1.6rem;
	color:#f17a3d;
	text-align:center;
	margin-top:-1.8rem;
}
.bottomBj{
	width:100%;
	height:10.4rem;
	position:relative;
}
.bottomBj img{
	width:100%;
	height:10.4rem;
	position:absolute;
}