</body>
<script>
	apiready=function(){
			api.openSlidLayout({
				type: 'left',		//划得方向左或者右
				leftEdge: 80,
				fixedPane: {		//固定页设置
					name: 'user',
					url: 'html/user.html',
					bgColor: '#fff',
					bounces: false,
					vScrollBarEnabled: true,
					hScrollBarEnabled: false
				},
				slidPane: {		//活动页设置
					name: 'main',
					url: 'html/main.html',
					bgColor: '#fff',
					bounces: false,
					vScrollBarEnabled: true,
					hScrollBarEnabled: false
				}
			}, function(ret, err) {
					api.toast({
				    msg: ret,
				    duration: 2000,
				    location: 'bottom'
				});
			});
		}
		
</script>
</html>