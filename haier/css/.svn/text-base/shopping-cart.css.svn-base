.delete:hover{
	background: #d71249;
	color: white;
	border: none;
}


#changebox1{
	display: block;
}
#changebox2{
    font-size: 12px;
    display: none;
}
.outbox1,.outbox2,.outbox3{
	border: 1px solid #ddd;
    padding: 0 10px 10px;
    background: #f4f5f7;
    border: 1px solid #ddd;
    border-radius: 3px;
    -moz-border-radius: 3px;
    -webkit-border-radius: 3px;
    margin-bottom: 20px;
    clear: both;
}
.middlebox{
	padding: 15px 30px 10px;
    background: #fff;
    border: 1px solid #ddd;
}
.middlebox>p{
	padding: 8px 0 5px;
}
.middlebox>p em{
	color: red;
}
.middlebox>p b{
	color: red;
}
.middlebox>p i{
	color: #909090;
}
.outbox1>p,.outbox2>p,.outbox3>p{
	line-height: 40px;
    font-size: 14px;
    font-weight: bold;
}

.middlebox select{
	width: 136px;
    height: 24px;
    border: 1px solid #aaa;
    padding: 1px;
    background-color: #fff;
}

