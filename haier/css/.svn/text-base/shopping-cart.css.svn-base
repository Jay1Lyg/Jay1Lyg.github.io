}

table{
    border-collapse: collapse;
    font-size: 12px;
}
tr th{
	text-align: left;
    border-bottom: solid 2px #a0a1a2;
    height: 38px;
    line-height: 38px;
}
.tr1{
    border-bottom: solid 1px #a0a1a2;
    line-height: 100px;
	
}
.tr2{
    border-bottom: solid 1px #a0a1a2;
    line-height: 24px;
	
}
.tr2 td{
	padding: 20px 0;
}
#th1{
	width: 370px;
    padding-left: 10px;
}
.th{
	width: 122px;
}
.th1{
	width: 154px;
}
td img{
	vertical-align: middle;
	font-weight: 700;
	display: inline-block;
}
td a{
	font-weight: 700;
    margin-left: 13px;
    width: 290px;
}
td button{
	font-weight: bold;
	padding: 3px;
	width: 50px;
}
.spanjian,.spanjian1,.spanjian2{
	border-left: 1px solid #ddd;
	float: left;
    cursor: pointer;
    font-weight: 700;
    font-size: 14px;
    height: 20px;
    width: 20px;
    line-height: 18px;
    text-align: center;
    border-bottom: 1px solid #ddd;
    border-top: 1px solid #ddd;
}
.spanjia,.spanjia1,.spanjia2{
	border-right: 1px solid #ddd;
	float: left;
    cursor: pointer;
    font-weight: 700;
    font-size: 14px;
    height: 20px;
    width: 20px;
    line-height: 18px;
    text-align: center;
    border-bottom: 1px solid #ddd;
    border-top: 1px solid #ddd;
}
td input{
	border: 1px solid #ddd;
    height: 18px;
    line-height: 20px;
    text-align: center;
    width: 42px;
    float: left;
}

