* {
    margin: 0;
    padding: 0;
}
#topNav {
    width: 100%;
    overflow: hidden;
    font: 16px/32px hiragino sans gb, microsoft yahei, simsun;
    border-bottom:1px solid #f8f8f8;
}

#topNav .swiper-slide span{
    font-size:14px;
    transition:all .3s ease;
    display:block;
}
#topNav .active span{
    transform:scale(1.1);
    color:#4da2ad;
}

#topNav1 {
    width: 100%;
    overflow: hidden;
    font: 16px/32px hiragino sans gb, microsoft yahei, simsun;
    border-bottom:1px solid #f8f8f8;
}
#topNav1 .swiper-slide {
    padding: 0 5px;
    letter-spacing:2px;
    width:50px;
    text-align:center;
}
#topNav1 .swiper-slide span{

    transition:all .3s ease;
    display:block;
}
#topNav1 .active span{
    transform:scale(1.1);
    color:#4da2ad;
}
img{
    width:100%;
    line-height:0;
}