/*弹出层*/
.dialogs{
    display: none;
    width: 80%;
    position: fixed;
    left: 50%;
    transform: translate(-50%, -120%);
    z-index: 101;
    animation:myfirst 1s;
    -webkit-animation:myfirst 1s;


}

@keyframes myfirst
{
    from {opacity:0;}
    to {opacity:1;}
}

@-webkit-keyframes myfirst /* Safari and Chrome */
{
    from {opacity:0;}
    to {opacity:1;}
}
