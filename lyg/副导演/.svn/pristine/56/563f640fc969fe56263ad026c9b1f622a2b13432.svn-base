<script>
    $(function(){
    var url="http://www.901vehicle.cn";
    var url3=url+"/WX/wx_allproductiontypes/1/null";
    console.log(url3);
        $.ajax({
            type : "get",
            url : url3,
            dataType : "jsonp",
            async:false,
