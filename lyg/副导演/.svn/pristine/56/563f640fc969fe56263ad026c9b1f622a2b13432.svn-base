            success : function(data){
                console.log(data);
                var str=$(".clearfix").html();
                for(var i=0;i<data.length;i++){
                    str+='<li  ><a href="javascript:void(0)">'+data[i].name+'</a></li>'
                }
                $(".clearfix").html(str);
                $('.wrapper').navbarscroll();

            },
            error:function(){
                alert('fail');
            }
         });
    });
</script>
