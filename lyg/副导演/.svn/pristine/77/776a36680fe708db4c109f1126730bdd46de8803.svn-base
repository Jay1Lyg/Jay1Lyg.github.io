header{
    z-index: 500;
    background-color:white;
    
    width: 100%;
}
.notice_class{
    margin: 0.1rem 0;
}
.notice_type{
    width: 100%;
    background: rgba(242, 242, 242, 1);
    height: 0.9rem;
}
.notice_type p{
    font-size: 0.28rem;
    color: #222222;
    line-height: 0.9rem;
    margin-left: 0.76rem;
    display: inline;
}

.jiantou{
    float: right;
    clear: both;
    height: 0.9rem;
}
.jiantou img{
    width: 0.5rem;
    margin: 0.2rem 0.62rem 0.2rem 0;
}
.jiantou img:last-child{
   display: none;
}

/*选显卡*/



/*演员样式*/
.display_experience{
    width: 100%;

}
.display_experience>div{
    clear: both;

}
.display_experience>div>div{
    float: left;
}
.display_experience>a>div{
    height: 3rem;
    padding: 0.1rem;
    background: white;
    margin-bottom: 0.4rem;
    clear: both;

}
.display_experience>a>div>div{
    float: left;
}
.photo_experience{
    width: 35%;
    height: 4rem;
    overflow:hidden;

}
.photo_experience img{
    width: 100%;
    /*height: 3.6rem;*/
    margin-top: 0.2rem;
}
.detail_experience{
    width: 55%;
    padding: 1%;
}
.zhixiang{
    width: 4%;
    height: 3.75rem;


}
.zhixiang>img{
    width: 0.4rem;
    display: block;
    margin-top:2rem;
}
.detail_experience img{
    float: right;
    width: 0.4rem;
}
.detail_experience ul{
    margin-top: 0.1rem;
    clear: both;

}
.detail_experience ul li{
    font-size: 14px;
    margin: 0.15rem 0;
    color: #696969;
}
.detail_experience ul li:first-child{
    font-size: 14px;
    margin: 0.1rem 0;
    color: black;

}
.detail_experience ul li:first-child span{
    color: black;

}
.detail_experience ul li span{
    color: #696969;
    width: 100%;
   font-size: 14px;
}

.detail_experience ul li span.types{
    color: #4da2ad;
    font-size: 14px;
    border: 1px solid #4da2ad;
    padding: 0.02rem 0.1rem;
    border-radius: 0.1rem;
}
.detail_experience ul p{
   font-size: 14px;
    color:#999 ;
}

/*角色信息=========================*/
.display_experience>div>div.about_me{
    float: right;
    margin: 0.2rem 0.83rem;
}
.display_experience>div>div.about_me button{
    width: 2.7rem;
    height: 1.25rem;
    background: #4da2ad;
    color: white;
    border: none;
    margin-left: 0.625rem;
    border-radius: 0.1rem;
}


.info_detile .true1{
    opacity: 0.2
}
.weui_cell .slideleft span.gua_active{
    display: none;
}

.add-actor{
    height: 0.7rem;
    width: 80%;
    background:white ;
    font-size: 14px;
    line-height: 0.7rem;
    border: 1px solid #f2f2f2;
    margin-top: 0.83rem;
    color: black;
}
.down{
 font-size: 14px;

}