
/*头部结束*/
section{
    height: 100%;
}
section .banner-1{

    position: relative;
}
section .banner-1 div{
    height: 44px;
    background-color: rgba(0,0,0,0.5);
    width: 100%;
    text-align: center;
    position: absolute;
    bottom: 0;

}
section .banner-1 div p{
    color: #ffffff;
    font-size: 15px;
    line-height: 44px;
}
section .banner-1 img{
    width: 100%;
    /*height: 400px;*/
}
.add-actor{
    height: 0.7rem;
    width: 80%;
    background:white ;
    font-size: 0.28rem;
    line-height: 0.7rem;
    border: 1px solid #f2f2f2;
    margin-top: 20px;
    color: black;
}

section .actor-data {
    color: #222222;
    line-height: 33px;
    font-size: 17px;
    /*color: #a0a0a0;*/
}
section input::placeholder{
    /*text-align: right;*/
    font-size: 14px;
    color: #a0a0a0;
}

section textarea::placeholder{
    font-size: 14px;
    color: #a0a0a0;
}
.common-input{
    font-size: 14px;
    color: #222222;
}
.add-live{
    border: 2px solid #4da2ad;
    color: #4da2ad;
    width: 340px;
    height: 44px;
    margin: 0 auto;
    font-size: 14px;
}

.title-height{
    height: 50px;
    line-height: 50px;
    font-size: 14px;
}

.look-over{
    background:#4da2ad ;
    height: 49px;
    width: 100%;
    font-size: 16px;
    line-height: 49px;
    position: fixed;
    bottom: 0;
    z-index: 999;
}
.look-over img{
    height: 10px;
    line-height: 49px;
    margin-right: 5px;
}
/*必填*/
.must{
    height: 10px;
    width: 10px;
}

/*性别样式*/

.option {
    font-size: 15px;
    text-align: right;
    color: #666;
}
.data{
    font-size: 15px;
    color: #a0a0a0;
}
/*input字体颜色*/
.font-color{
    color: #666666;
    font-size: 14px;
}

.pic img{
    margin: 10px 0 0 0px;

}
